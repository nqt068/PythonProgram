print("Enter the degree of polynomial:", end = " ")
n = int(input())
a = []
for i in range(0, n+1):
    print(f"\tEnter coefficent of X^{i}:", end = " ")
    a.append(float(input()))
print(a[0], end = " ")
for i in range(1, n+1):
    if (a[i] == 0):
        continue
    else:
        if (a[i] < 0):
            print(f"-{-a[i]}*X^{i}", end = " ")
        else:
            print(f"+{a[i]}*X^{i}", end = " ")
